/*框架样式*/
html{
	max-height: auto;
	max-width: 1000px;
	width:expression(this.width>400?"400px":this.width);
	height:expression(this.height>350?"350px":this.height);
	overflow:hidden;
	border:0;
	padding-left: 190px;	
}
.main{
	text-align: center;
	background-color: #FFFACD;
}
/*头部样式设置*/
.top{
	background-color: #9ACD32;
	overflow: auto;
	max-height: 250px;
	height: 70px;
}
.top_left{
	float: left;
	padding-top: 30px;
	font-size: 20px
}
.top_middle,.top_right,.top_search{
	float: left;
	padding-top: 30px;
	padding-left: 10px;
}
.top_riqi{
	float: left;
	padding-top: 30px;
	padding-left: 10px;
}
